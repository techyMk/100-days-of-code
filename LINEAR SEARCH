//Program to search an element in an array

#include <stdio.h>

int main(void) {
	int n=0,x=0,i,flag=0;
	scanf("%d",&n);
	int arr[n];
	for(i=0;i<n;i++)
	{
	    scanf("%d",&arr[i]);
	}
	scanf("%d",&x);
	for(i=0;i<n;i++)
	{
	   if(arr[i]==x)
	    {
	       flag++;
	    }
	}    
	if(flag>=1)
	{
	     printf("YES");
	}
	else
	{
	printf("NO");
	}
	return 0;
}

