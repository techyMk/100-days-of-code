//Program to find the minimum number of coins Chef needs to pay

#include <iostream>
using namespace std;

int main() {
	int t,x;
	cin>>t;
	while(t--)
	{
	    cin>>x;
	    if(x%10==0)
	    {
	        cout<<x/10<<"\n";
	    }
	    else if(x%5==0)
	    {
	        cout<<x/10+1<<"\n";
	    }
	    else
        {
            cout<<"-1"<<"\n";
        }
	}
	return 0;
}
