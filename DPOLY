//Program to find the degree of the polynomial

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int ar[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>ar[i];
	    }
	    for(int i=n-1;i>=0;i--)
	    {
	        if(ar[i]!=0)
	        {
	            cout<<(i)<<endl;
	            break;
	        }
	    }
	    
	}
	return 0;
}
