//Program to find the second largest of three numbers

#include <iostream>
using namespace std;

int main() {
	int n,x,y,z;
	cin>>n;
	while(n--)
	{
	    cin>>x>>y>>z;
	    if(x>=y && x>=z)
	    {
	        if(y>=z)
	        {
	        cout<<y<<endl;
	        }
	        else
            {
                cout<<z<<endl;
            }
	    }
	    else if(y>=x && y>=z)
	    {
	        if(x>=z)
	        {
	        cout<<x<<endl;
	        }
	        else
            {
                cout<<z<<endl;
            }
	    }
	    else
        {
            if(x>=y)
            {
            cout<<x<<endl;
            }
            else
            cout<<y<<endl;
        }
	}
	return 0;
}
