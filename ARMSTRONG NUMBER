//Program to check whether the given number is an armstrong number or not

string armstrongNumber(int n){
        string ans;
        int sum=0;
	    int temp=n;
	    while(temp!=0){
	        int r=temp%10;
		    sum=sum+r*r*r;
		    temp=temp/10;
	    }
	    if(sum==n){
	        ans="Yes";
		    return ans;
	    }
	    else{
	        ans="No";
		    return ans;
	    }
    }
